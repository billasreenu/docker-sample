            }
        }
        stage('Stage 2') {
            steps {
                echo 'Multibranch pipeline Hello world! 2' 
            }
        }
        stage('Stage 3') {
            steps {
                echo 'Multibranch pipeline Hello world! 3' 
            }
        }
    }
}